       RowBox[{"\[LeftAngleBracket]", 
        SubscriptBox["C\[Degree]", "p"], "\[RightAngleBracket]"}], "H"]}], 
     "=", 
     RowBox[{"R", "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[CapitalSigma]n", "i"], 
        SubscriptBox["A", 
         RowBox[{"i", " "}]]}], "+", " ", 
       RowBox[{
        FractionBox[
         RowBox[{
          SubscriptBox["\[CapitalSigma]n", "i"], 
          SubscriptBox["B", 
           RowBox[{"i", " "}]]}], "2"], 
        RowBox[{"(", 
         RowBox[{"T", " ", "-", " ", 
          SubscriptBox["T", "o"]}], ")"}]}], " ", "+", " ", 
       RowBox[{
        FractionBox[
         RowBox[{
          SubscriptBox["\[CapitalSigma]n", "i"], 
          SubscriptBox["C", 
           RowBox[{"i", " "}]]}], "3"], 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox["T", 
           RowBox[{"2", " "}]], "+", " ", 
          SuperscriptBox[
           SubscriptBox["T", "o"], "2"], "+", 
          SubscriptBox["TT", "o"]}], ")"}]}], " ", "+", " ", 
       RowBox[{
        SubscriptBox["\[CapitalSigma]n", "i"], 
        SubscriptBox["D", "i"]}]}]}]}]}], TraditionalForm]],
  FontSize->24,ExpressionUUID->"9a4de6a4-f736-4aae-bc86-24d66a8dbd52"],
